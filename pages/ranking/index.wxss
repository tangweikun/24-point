.scroll-wrapper {
  margin-top: 250rpx;
  height: 100%;
  width: 100%;
}

.home-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #6379cc;
  /* padding-top: 50rpx; */
}

.tab-wrapper {
  display: flex;
  width: 100%;
}

.tab-left {
  height: 120rpx;
  line-height: 120rpx;
  text-align: center;
  width: 50%;
  background: #5bc887;
  box-sizing: border-box;
  border-right: 1rpx solid rgba(255, 255, 255, 0.4);
  /* border-top-left-radius: 10rpx; */
}

.tab-active {
  color: #ffffff;
}

.tab-right {
  height: 120rpx;
  line-height: 120rpx;
  text-align: center;
  width: 50%;
  background: #5bc887;
  box-sizing: border-box;
  border-left: 1rpx solid rgba(255, 255, 255, 0.4);
  /* border-top-right-radius: 10rpx; */
}

.tab-normal {
  color: rgba(255, 255, 255, 0.4);
}

.ad-wrapper {
  position: fixed;
  bottom: 20rpx;
}

.my-rank-wrapper {
  width: 100%;
}

.fixed-wrapper {
  position: fixed;
  z-index: 1000;
  width: 100%;
}
